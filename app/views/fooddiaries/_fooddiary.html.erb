<!--<tbody>-->
  <tr>
    <td><%= fooddiary.timing %></td>
    <td><%= fooddiary.fooddate.foodname %></td>
    <td><%= fooddiary.gram.ceil %></td>
    <td><%= fooddiary.unit %></td>
    <td><%= fooddiary.kcal.ceil(2).to_f %>kcal</td>
    <td><%= fooddiary.protein.ceil(2).to_f %>g</td>
    <td><%= fooddiary.fat.ceil(2).to_f %>g</td>
    <td><%= fooddiary.carbo.ceil(2).to_f %>g</td>
    <td class="td-actions text-right">
        <%= link_to edit_fooddiary_path(fooddiary), class: "btn btn-link btn-warning btn-just-icon edit" do %>
          <i class="material-icons">edit</i>
        <% end %>
        <%= link_to fooddiary_path(fooddiary), method: :delete, remote: true, class: "btn btn-link btn-danger btn-just-icon remove" do %>
            <i class="material-icons">close</i>
        <% end %>
    </td>
  </tr>
<!--</tbody>-->