<!--<tbody>-->
  <tr>
    <td><%= fooddiary.timing %></td>
    <td><%= fooddiary.fooddate.foodname %></td>
    <td><%= fooddiary.gram %></td>
    <td><%= fooddiary.unit %></td>
    <td><%= fooddiary.kcal %>kcal</td>
    <td><%= fooddiary.protein %>g</td>
    <td><%= fooddiary.fat %>g</td>
    <td><%= fooddiary.carbo %>g</td>
    <td class="td-actions text-right">
        <%= link_to edit_fooddiary_path(fooddiary), class: "btn btn-link btn-warning btn-just-icon edit" do %>
          <i class="material-icons">edit</i>
        <% end %>
        <%= link_to fooddiary_path(fooddiary), method: :delete, remote: true, class: "btn btn-link btn-danger btn-just-icon remove" do %>
            <i class="material-icons">close</i>
        <% end %>
    </td>
  </tr>
<!--</tbody>-->